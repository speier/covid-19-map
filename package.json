{
  "name": "covid-19-map",
  "version": "0.1.0",
  "description": "COVID-19 world map based on data by Johns Hopkins CSSE",
  "keywords": ["covid-19"],
  "license": "ISC",
  "main": "src/index.js",
  "dependencies": {
    "@antv/l7-react": "2.1.3",
    "papaparse": "5.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.1.0",
    "autoprefixer": "9.7.4",
    "postcss-cli": "7.1.0",
    "tailwindcss": "1.2.0",
    "typescript": "3.3.3"
  },
  "scripts": {
    "build:styles": "postcss src/styles/main.css -o src/styles/build/main.css",
    "prestart": "npm run build:styles",
    "start": "react-scripts start",
    "prebuild": "NODE_ENV=production npm run build:styles",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
